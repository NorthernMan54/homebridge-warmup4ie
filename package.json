{
  "name": "homebridge-warmup4ie",
  "version": "0.1.0",
  "description": "Warmup4IE Thermostat Plugin Homebridge",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint --max-warnings=0 .",
    "lint:fix": "eslint --fix --max-warnings=0 .",
    "watch": "nodemon",
    "test": "jest --detectOpenHandles",
    "test-coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NorthernMan54/homebridge-warmup4ie.git"
  },
  "keywords": [
    "warmup4ie",
    "homebridge-plugin"
  ],
  "engines": {
    "node": ">=4.4.5",
    "homebridge": ">=0.4.16"
  },
  "dependencies": {
    "debug": "^4.4.0",
    "fakegato-history": "^0.6.5",
    "homebridge-lib": "^6.0.1",
    "moment": "^2.30.1",
    "request": "^2.34.0",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "eslint": "^8.57.1",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-jest": "^28.8.3",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7"
  },
  "author": "NorthernMan54",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/NorthernMan54/homebridge-warmup4ie/issues"
  },
  "homepage": "https://github.com/NorthernMan54/homebridge-warmup4ie#readme",
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "js",
    "ignore": [],
    "exec": "~/npm/bin/homebridge -U ./test/hbConfig -Q -T -D -P .",
    "signal": "SIGTERM",
    "env": {
      "NODE_OPTIONS": "--trace-warnings"
    }
  }
}